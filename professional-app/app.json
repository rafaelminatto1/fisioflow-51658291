{
  "expo": {
    "name": "FisioFlow Pro",
    "slug": "vite_react_shadcn_ts",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "fisioflowpro",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1E40AF"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fisioflow.professionals",
      "buildNumber": "1",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "NSCameraUsageDescription": "Permitir acesso a camera para fotos de pacientes e exercicios",
        "NSPhotoLibraryUsageDescription": "Permitir acesso a galeria para selecionar fotos",
        "NSMicrophoneUsageDescription": "Permitir acesso ao microfone para gravar videos de exercicios"
      },
      "config": {
        "usesNonExemptEncryption": false
      },
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1E40AF"
      },
      "package": "com.fisioflow.professional",
      "googleServicesFile": "./google-services.json",
      "runtimeVersion": "1.0.0"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-notifications"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8e006901-c021-464d-bbcd-96d821ab62d0"
      }
    },
    "owner": "rafaelminatto",
    "updates": {
      "url": "https://u.expo.dev/8e006901-c021-464d-bbcd-96d821ab62d0"
    }
  }
}
