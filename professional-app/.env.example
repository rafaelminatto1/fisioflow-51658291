# Firebase Configuration
# Get these values from Firebase Console > Project Settings > Your apps > Config
EXPO_PUBLIC_FIREBASE_API_KEY=your_api_key_here
EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
EXPO_PUBLIC_FIREBASE_PROJECT_ID=your_project_id
EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET=your_project_id.appspot.com
EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
EXPO_PUBLIC_FIREBASE_APP_ID=your_app_id

# Optional: Analytics
EXPO_PUBLIC_FIREBASE_MEASUREMENT_ID=your_measurement_id

# Sentry Configuration
# Organization slug and project name (for app.json and sourcemaps upload)
SENTRY_ORG=activity-fisioterapia-rg
SENTRY_PROJECT=fisioflow-professional-app-tm

# DSN do Sentry - Configure via vari√°vel de ambiente
# Get your DSN from Sentry dashboard: https://sentry.io/projects/
EXPO_PUBLIC_SENTRY_DSN=https://3753422576dd45c58ad5d1917ea8cd26@o4510069182955520.ingest.us.sentry.io/4510933740158976

# Sentry Auth Token for source maps upload
# Get from: https://sentry.io/settings/account/api/auth-tokens/
# Required scopes: project:read, project:releases, org:read
SENTRY_AUTH_TOKEN=your_sentry_auth_token_here

# Environment (development, staging, production)
EXPO_PUBLIC_ENVIRONMENT=development
