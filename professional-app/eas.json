{
  "cli": {
    "version": ">= 12.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://fisioflow.com.br/api",
        "EXPO_PUBLIC_ENVIRONMENT": "development",
        "SENTRY_ORG": "activity-fisioterapia-rg",
        "SENTRY_PROJECT": "fisioflow-professional-app-tm"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": false,
        "autoIncrement": true
      },
      "android": {
        "buildType": "apk",
        "autoIncrement": true
      },
      "credentialsSource": "remote",
      "env": {
        "EXPO_PUBLIC_API_URL": "https://fisioflow.com.br/api",
        "EXPO_PUBLIC_ENVIRONMENT": "staging",
        "SENTRY_ORG": "activity-fisioterapia-rg",
        "SENTRY_PROJECT": "fisioflow-professional-app-tm"
      },
      "cache": {
        "disabled": false
      }
    },
    "production": {
      "autoIncrement": true,
      "ios": {
        "autoIncrement": true
      },
      "android": {
        "autoIncrement": true
      },
      "credentialsSource": "remote",
      "env": {
        "EXPO_PUBLIC_API_URL": "https://fisioflow.com.br/api",
        "EXPO_PUBLIC_ENVIRONMENT": "production",
        "SENTRY_ORG": "activity-fisioterapia-rg",
        "SENTRY_PROJECT": "fisioflow-professional-app-tm"
      },
      "cache": {
        "disabled": false
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "rafael.minatto@yahoo.com.br",
        "appName": "FisioFlow Pro",
        "ascAppId": "6504123456",
        "appleTeamId": "G7FDW933SF"
      }
    }
  }
}
