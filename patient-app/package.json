{
  "name": "fisioflow-ios",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "private": true,
  "devDependencies": {
    "@playwright/test": "^1.50.0",
    "playwright": "^1.50.0"
  },
  "scripts": {
    "start": "expo start",
    "ios": "expo start --ios",
    "android": "expo start --android",
    "build:dev": "eas build --profile development --platform ios",
    "build:preview": "eas build --profile preview --platform ios",
    "build:prod": "eas build --profile production --platform ios",
    "submit": "eas submit --platform ios",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test --config=playwright.config.ts",
    "test:e2e:headed": "playwright test --config=playwright.config.ts --headed",
    "test:e2e:debug": "playwright test --config=playwright.config.ts --debug"
  }
}
