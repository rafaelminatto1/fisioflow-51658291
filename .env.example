# FisioFlow - Configurações de Ambiente

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Analytics Configuration
VITE_GA_MEASUREMENT_ID=G-XXXXXXXXXX
VITE_ANALYTICS_ENDPOINT=https://your-domain.com/api/analytics
VITE_ANALYTICS_ENABLED=true

# Environment
VITE_APP_ENV=development
VITE_APP_VERSION=1.0.0

# API Configuration
VITE_API_BASE_URL=https://your-domain.com/api

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_MONITORING=true
VITE_ENABLE_ERROR_TRACKING=true

# Performance
VITE_ENABLE_LAZY_LOADING=true
VITE_ENABLE_PRELOAD=

# MCP Configuration
GITHUB_API_KEY=your_github_api_key_here

# ========================================
# Feature Flags (via Environment Variables)
# ========================================
# Use these ALTERNATIVE to Edge Config if needed
# Set to 'true' to enable, 'false' to disable, or leave empty for defaults

# Dashboard
VITE_FEATURE_NEW_DASHBOARD=false

# AI Features
VITE_FEATURE_AI_TRANSCRIPTION=true
VITE_FEATURE_AI_CHATBOT=true
VITE_FEATURE_AI_EXERCISE_SUGGESTIONS=true

# Clinical Features
VITE_FEATURE_DIGITAL_PRESCRIPTION=true
VITE_FEATURE_PAIN_MAP_V2=false
VITE_FEATURE_SOAP_RECORDS_V2=false

# Analytics
VITE_FEATURE_ADVANCED_ANALYTICS=true
VITE_FEATURE_PATIENT_REPORTS_V2=false

# Integration
VITE_FEATURE_WHATSAPP_NOTIFICATIONS=true
VITE_FEATURE_GOOGLE_CALENDAR_SYNC=true

# System
VITE_FEATURE_MAINTENANCE_MODE=false
VITE_FEATURE_BETA_FEATURES=false

# ========================================
# Pro Integrations - New Environment Variables
# ========================================

# Vercel KV Configuration (Redis-based caching)
KV_URL=your-kv-redis-url
KV_REST_API_URL=your-kv-rest-api-url
KV_REST_API_TOKEN=your-kv-rest-api-token
KV_REST_API_READ_ONLY_TOKEN=your-kv-read-only-token

# Vercel Edge Config (Feature flags)
EDGE_CONFIG=https://edge-config.vercel.com/your-edge-config-id

# OpenAI Configuration (Embeddings and AI features)
OPENAI_API_KEY=your-openai-api-key-here

# Cron Job Security
CRON_SECRET=generate-a-random-secret-here-use-openssl-rand-base64-32

# Supabase Service Role Key (for migrations - keep secret!)
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key-here

# Stripe Webhook Secret
STRIPE_WEBHOOK_SECRET=whsec_your-stripe-webhook-secret

# WhatsApp Configuration
WHATSAPP_VERIFY_TOKEN=your-whatsapp-verify-token

# Google Calendar
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
GOOGLE_CALENDAR_WEBHOOK_SECRET=your-google-calendar-webhook-secret

# Resend Email
RESEND_API_KEY=re_your-resend-api-key-here