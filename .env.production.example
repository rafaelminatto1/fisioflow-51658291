# FisioFlow Production Environment Configuration
# Copy this file to .env.production and fill in the actual values

# Supabase Configuration
VITE_SUPABASE_URL=https://your-project-id.supabase.co
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key

# Push Notifications Configuration
VITE_VAPID_PUBLIC_KEY=your-vapid-public-key
VAPID_PRIVATE_KEY=your-vapid-private-key

# Application Configuration
VITE_APP_ENV=production
VITE_APP_NAME=FisioFlow
VITE_APP_VERSION=1.0.0

# Performance Configuration
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_BATCH_NOTIFICATION_SIZE=100
VITE_BATCH_WINDOW_MS=30000

# Security Configuration
VITE_ENABLE_SECURITY_HEADERS=true
VITE_CSP_ENABLED=true

# Analytics Configuration (Optional)
VITE_ANALYTICS_ENABLED=true
VITE_ANALYTICS_ID=your-analytics-id

# Error Reporting (Optional)
VITE_SENTRY_DSN=your-sentry-dsn
VITE_ERROR_REPORTING_ENABLED=true

# Feature Flags
VITE_ENABLE_PUSH_NOTIFICATIONS=true
VITE_ENABLE_NOTIFICATION_ANALYTICS=true
VITE_ENABLE_BATCH_OPTIMIZATION=true

# Database Configuration (for Edge Functions)
DATABASE_URL=postgresql://postgres:password@db.your-project-id.supabase.co:5432/postgres
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Notification Service Configuration
NOTIFICATION_RETRY_ATTEMPTS=3
NOTIFICATION_RETRY_DELAY=5000
NOTIFICATION_BATCH_SIZE=100
NOTIFICATION_QUIET_HOURS_DEFAULT_START=22:00
NOTIFICATION_QUIET_HOURS_DEFAULT_END=08:00

# Performance Monitoring
PERFORMANCE_METRICS_RETENTION_DAYS=30
SYSTEM_HEALTH_CHECK_INTERVAL=30000
BATCH_OPTIMIZATION_INTERVAL=3600000

# Security Settings
CORS_ALLOWED_ORIGINS=https://your-domain.com,https://www.your-domain.com
RATE_LIMIT_REQUESTS_PER_MINUTE=100
RATE_LIMIT_BURST_SIZE=20

# Logging Configuration
LOG_LEVEL=info
LOG_FORMAT=json
ENABLE_REQUEST_LOGGING=true

# Cache Configuration
CACHE_TTL_SECONDS=300
CACHE_MAX_SIZE=1000

# Notification Templates
DEFAULT_NOTIFICATION_ICON=/icons/notification-icon-192.png
DEFAULT_NOTIFICATION_BADGE=/icons/notification-badge-72.png
NOTIFICATION_CLICK_URL=https://your-domain.com

# PWA Configuration
PWA_THEME_COLOR=#1976d2
PWA_BACKGROUND_COLOR=#ffffff
PWA_DISPLAY=standalone

# Development/Debug (set to false in production)
VITE_DEBUG_MODE=false
VITE_ENABLE_CONSOLE_LOGS=false
VITE_MOCK_NOTIFICATIONS=false