{
  "name": "fisioflow-functions",
  "version": "1.0.0",
  "description": "Cloud Functions for FisioFlow",
  "main": "lib/src/index.js",
  "scripts": {
    "build": "tsc && node scripts/fix-build.js",
    "build:watch": "tsc --watch",
    "serve": "npm run build && firebase emulators:start --only functions",
