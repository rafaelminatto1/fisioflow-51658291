{
  "expo": {
    "name": "FisioFlow Pacientes",
    "slug": "fisioflow-patients",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "fisioflowpatient",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#3B82F6"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "backgroundColor": "#FFFFFF",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fisioflow.patients",
      "userInterfaceStyle": "light"
    },
    "android": {
      "package": "com.fisioflow.patients",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#3B82F6"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "web": {
      "bundler": "metro"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8e006901-c021-464d-bbcd-96d821ab62d0"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/8e006901-c021-464d-bbcd-96d821ab62d0"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}