{
  "expo": {
    "name": "FisioFlow Profissionais",
    "slug": "fisioflow-professionals",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "fisioflowprofessional",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1E293B"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fisioflow.professionals",
      "buildNumber": "1",
      "infoPlist": {
        "NSCameraUsageDescription": "Necessário para registrar avaliações e exercícios",
        "NSPhotoLibraryUsageDescription": "Necessário para acessar fotos de pacientes"
      },
      "googleServicesFile": "./GoogleService-Info.plist",
      "config": {
        "googleSignIn": {
          "reservedClientId": "CLIENT_ID_REVERSO_DO_FIREBASE"
        }
      },
    }
  },
  "android": {
    "package": "com.fisioflow.professionals",
    "googleServicesFile": "./google-services.json"
  },
  "plugins": [
    "@expo/config-plugins",
    [
      "@expo-google-fonts/inter",
      {
        "fonts": [
          "inter_400",
          "inter_500",
          "inter_600",
          "inter_700"
        ]
      }
    ],
    [
      "expo-notifications",
      {
        "icon": "./assets/notification-icon.png",
        "color": "#1E293B"
      }
    ]
  ],
  "experiments": {
    "typedRoutes": true
  },
  "extra": {
    "router": {
      "origin": false
    },
    "eas": {
      "projectId": "8e006901-c021-464d-bbcd-96d821ab62d0"
    }
  },
  "updates": {
    "url": "https://u.expo.dev/será-adicionado-pelo-eas"
  },
  "runtimeVersion": {
    "policy": "appVersion"
  }
}
}