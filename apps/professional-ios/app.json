{
  "expo": {
    "name": "FisioFlow Profissionais",
    "slug": "fisioflow-professionals",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "fisioflowprofessional",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1E293B"
    },
    "assetBundlePatterns": ["**/*"],
    "backgroundColor": "#FFFFFF",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fisioflow.professionals",
      "buildNumber": "1",
      "userInterfaceStyle": "automatic",
      "infoPlist": {
        "NSCameraUsageDescription": "Necessario para registrar avaliacoes e exercicios",
        "NSPhotoLibraryUsageDescription": "Necessario para acessar fotos de pacientes"
      }
    },
    "android": {
      "package": "com.fisioflow.professionals",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#1E293B"
      }
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#1E293B"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "web": {
      "bundler": "metro"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "fisioflow-professionals"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/fisioflow-professionals"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
